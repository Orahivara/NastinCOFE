<Window x:Class="NastinCOFE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NastinCOFE"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="login" HorizontalAlignment="Left" Margin="308,177,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="password" HorizontalAlignment="Left" Margin="308,217,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" />
        <Label x:Name="AutName" Content="Вход" HorizontalAlignment="Left" Margin="308,118,0,0" VerticalAlignment="Top" Height="54" Width="180"/>
        <Button x:Name="signin" Content="Зарегистрироваться" HorizontalAlignment="Left" Margin="280,333,0,0" VerticalAlignment="Top" />
        <Button x:Name="_in" Content="Войти" HorizontalAlignment="Left" Margin="414,333,0,0" VerticalAlignment="Top" Click="_in_Click"/>
    </Grid>
</Window>